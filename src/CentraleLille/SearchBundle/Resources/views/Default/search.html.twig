

<div id="page-wrapper">
            <div class="row">
                <div class="col-lg-12">
                    <h1 class="page-header">Ajouter un souhait</h1>
                </div>
                <!-- /.col-lg-12 -->
            </div>
            <!-- /.row -->
            <div class="row">
                <div class="col-lg-12">
                    <div class="panel panel-default">
                        <div class="panel-heading">

                        </div>
                        <div class="panel-body">
                            <div class="row">
                            <div class="col-lg-12">
                                  {{ form_start(form, {'attr': {'class': 'form-horizontal'}}) }}

                                   {{ form_errors(form) }}



                                        <div class="form-group">
                                            {{ form_label(form.username, "Recherche", {'label_attr': {'class': 'col-sm-6 control-label'}}) }}
                                            {{ form_errors(form.username) }}
                                            {{ form_widget(form.username, {'attr': {'class': 'form-control'}})}}
                                        </div>

                                        {{ form_end(form) }}





                                </div>

                                <div class="col-lg-12">

                                <table class="table table-striped table-bordered table-hover" id="dataTables-example">
                                    <thead>
                                        <tr>
                                            <th>id</th>
                                            <th>username</th>
                                            <th>mail</th>

                                        </tr>
                                    </thead>
                                    <tbody>

                                        {% for Liste in results %}
                                          <tr class="odd gradeX">
                                          <td>
                                          {{ Liste.id }}
                                          </td>

                                        <td>{{ Liste.username }}</td>
                                           <td>{{ Liste.email }}</td>







                                          {%  endfor %}
                                        </tr>

                                    </tbody>

                                </table>






                                </div>


                            </div>
                            <!-- /.row (nested) -->
                        </div>
                        <!-- /.panel-body -->
                    </div>
                    <!-- /.panel -->
                </div>
                <!-- /.col-lg-12 -->
            </div>
            <!-- /.row -->
        </div>
